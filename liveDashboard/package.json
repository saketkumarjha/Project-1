{
  "name": "hospital-live-dashboard",
  "version": "1.0.0",
  "description": "Real-time dashboard service for hospital administration",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs live-dashboard"
  },
  "keywords": [
    "hospital",
    "dashboard",
    "real-time",
    "websocket",
    "redis",
    "mongodb"
