  "author": "Hospital Management Team",
  "license": "MIT",
  "dependencies": {
    "socket.io": "^4.7.2",
    "redis": "^4.6.7",
    "mongoose": "^7.4.0",
    "jsonwebtoken": "^9.0.1",
    "dotenv": "^16.3.1",
    "winston": "^3.10.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "rate-limiter-flexible": "^2.4.2",
    "node-cron": "^3.0.2",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.4.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/jest": "^29.5.3",
    "@types/node-cron": "^3.0.8",
    "@types/babel__core": "^7.20.1",
    "typescript": "^5.1.6",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hospital/live-dashboard.git"
